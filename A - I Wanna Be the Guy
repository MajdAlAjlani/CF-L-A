#include <iostream>

using namespace std;

int main()
{
    int t ,n,m;
    cin>>t;

    int a[t+1];
    for(int i=0 ; i<=t ; ++i) a[i]=0;
    cin>>n;
    int x[n];
    for(int i=0 ; i<n ; i++){cin>>x[i];a[x[i]]++;}
    cin>>m;
    int y[m];
    for(int i=0 ; i<m ; i++){cin>>y[i];a[y[i]]++;}


    for(int i =1 ; i<=t ; i++)
    if(a[i]==0){cout<<"Oh, my keyboard!"<<endl;return 0;}

    cout<<"I become the guy."<<endl;
}
